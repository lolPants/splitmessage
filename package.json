{
  "name": "@lolpants/splitmessage",
  "version": "0.1.0",
  "packageManager": "yarn@3.2.3",
  "type": "module",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "sideEffects": false,
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "prepack": "yarn run build",
    "prepublish": "yarn run test",
    "test": "yarn run check && yarn run lint",
    "check": "yarn run compile --noEmit",
    "lint": "xo ./src",
    "clean": "rimraf lib",
    "compile": "tsc",
    "build": "yarn run clean && yarn run compile",
    "dev": "yarn run clean && yarn run compile --watch"
  },
  "repository": "git@github.com:lolPants/splitmessage.git",
  "author": "Jack Baron <jackmbaron@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@types/node": "^18.7.18",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.3",
    "xo": "^0.52.3"
  }
}
